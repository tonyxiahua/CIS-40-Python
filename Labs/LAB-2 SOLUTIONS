>>> def powers(num):
...     print(num**0)
...     print(num**1)
...     print(num**2)
...     print(num**3)
...     print(num**4)
...     print(num**5)
...     print(num**6)
...     print(num**7)
...     print(num**8)
...     print(num**9)
